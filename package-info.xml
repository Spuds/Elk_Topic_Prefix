	<id>emanuele:TopicsPrefix</id>
	<name>Topics Prefix</name>
	<version>0.0.2</version>
	<type>modification</type>
	<install for="1.0 - 1.0.99">
		<require-file name="TopicPrefix.integrate.php" destination="SOURCEDIR" />
		<require-file name="ManagePrefix.controller.php" destination="ADMINDIR" />
		<require-file name="Prefix.controller.php" destination="CONTROLLERDIR" />
		<require-file name="TopicPrefix.class.php" destination="SUBSDIR" />
		<require-file name="TopicPrefixPxCRUD.class.php" destination="SUBSDIR" />
		<require-file name="TopicPrefixTcCRUD.class.php" destination="SUBSDIR" />
		<require-file name="TopicPrefix.subs.php" destination="SUBSDIR" />
		<require-file name="TopicPrefix.english.php" destination="LANGUAGEDIR/english" />
		<require-file name="TopicPrefix.template.php" destination="THEMEDIR" />
		<require-file name="TopicPrefix.css" destination="THEMEDIR/css" />
		<code>install.php</code>
	</install>
	<uninstall for="1.0 - 1.0.99">
		<remove-file name="SOURCEDIR/TopicPrefix.integrate.php" />
		<remove-file name="ADMINDIR/ManagePrefix.controller.php" />
		<remove-file name="CONTROLLERDIR/Prefix.controller.php" />
		<remove-file name="SUBSDIR/TopicPrefix.class.php" />
		<remove-file name="SUBSDIR/TopicPrefixPxCRUD.class.php" />
		<remove-file name="SUBSDIR/TopicPrefixTcCRUD.class.php" />
		<remove-file name="SUBSDIR/TopicPrefix.subs.php" />
		<remove-file name="LANGUAGEDIR/english/TopicPrefix.english.php" />
		<remove-file name="THEMEDIR/TopicPrefix.template.php" />
		<remove-file name="THEMEDIR/css/TopicPrefix.css" />
		<code>install.php</code>
	</uninstall>
</package-info>